{
	"name": "dataflowtitleBasics",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "TitleBasicsParquet",
						"type": "DatasetReference"
					},
					"name": "sourcetitlebasics"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTableTitleBasics",
						"type": "DatasetReference"
					},
					"name": "sinktitleBasics"
				}
			],
			"transformations": [
				{
					"name": "derivedColumnTitleBasics"
				},
				{
					"name": "selecttitleBasics"
				}
			],
			"scriptLines": [
				"parameters{",
				"     jobid as string (uuid())",
				"}",
				"source(output(",
				"          tconst as string,",
				"          titleType as string,",
				"          primaryTitle as string,",
				"          originalTitle as string,",
				"          isAdult as long,",
				"          startYear as long,",
				"          endYear as long,",
				"          runtimeMinutes as long,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> sourcetitlebasics",
				"sourcetitlebasics derive(DI_JOB_ID = $jobid,",
				"          DI_LOAD_DT = currentTimestamp(),",
				"     partitionBy('hash', 1)) ~> derivedColumnTitleBasics",
				"derivedColumnTitleBasics select(mapColumn(",
				"          TCONST = tconst,",
				"          TITLETYPE = titleType,",
				"          PRIMARYTITLE = primaryTitle,",
				"          ORIGINALTITLE = originalTitle,",
				"          ISADULT = isAdult,",
				"          STARTYEAR = startYear,",
				"          ENDYEAR = endYear,",
				"          RUNTIMEMINUTES = runtimeMinutes,",
				"          GENRES = genres,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DT",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> selecttitleBasics",
				"selecttitleBasics sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TITLE_BASICS_SK as decimal(38,0),",
				"          TCONST as string,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as decimal(10,0),",
				"          STARTYEAR as decimal(38,0),",
				"          ENDYEAR as decimal(38,0),",
				"          RUNTIMEMINUTES as decimal(38,0),",
				"          GENRES as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     stageInsert: true,",
				"     mapColumn(",
				"          TCONST,",
				"          TITLETYPE,",
				"          PRIMARYTITLE,",
				"          ORIGINALTITLE,",
				"          ISADULT,",
				"          STARTYEAR,",
				"          ENDYEAR,",
				"          RUNTIMEMINUTES,",
				"          GENRES,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DATE = DI_LOAD_DT",
				"     )) ~> sinktitleBasics"
			]
		}
	}
}