{
	"name": "dataflowfactmovietest",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SnowflakeTableTitleBasics",
						"type": "DatasetReference"
					},
					"name": "sourcetitlebasicsstage"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeTableStageTitleAkas",
						"type": "DatasetReference"
					},
					"name": "source2titleakasstage"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeTableTitlePrincipals",
						"type": "DatasetReference"
					},
					"name": "source3titleprincipalsstage"
				},
				{
					"dataset": {
						"referenceName": "SnowflakeTable_IMDB",
						"type": "DatasetReference"
					},
					"name": "source4titleratingstage"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "lookup1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TITLE_BASICS_SK as decimal(38,0),",
				"          TCONST as string,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as decimal(10,0),",
				"          STARTYEAR as decimal(38,0),",
				"          ENDYEAR as decimal(38,0),",
				"          RUNTIMEMINUTES as decimal(38,0),",
				"          GENRES as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourcetitlebasicsstage",
				"source(output(",
				"          TITLE_AKAS_SK as decimal(38,0),",
				"          TITLEID as string,",
				"          ORDERING as decimal(38,0),",
				"          TITLE as string,",
				"          REGION as string,",
				"          LANGUAGE as string,",
				"          TYPES as string,",
				"          ATTRIBUTES as string,",
				"          ISORIGINALTITLE as decimal(38,0),",
				"          FILENAME as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> source2titleakasstage",
				"source(output(",
				"          TITLE_PRINCIPALS_SK as decimal(38,0),",
				"          TCONST as string,",
				"          ORDERING as decimal(38,0),",
				"          NCONST as string,",
				"          CATEGORY as string,",
				"          JOB as string,",
				"          CHARACTERS as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> source3titleprincipalsstage",
				"source(output(",
				"          TITLE_RATING_SK as decimal(38,0),",
				"          TCONST as string,",
				"          AVERAGERATING as double,",
				"          NUMVOTES as decimal(38,0),",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> source4titleratingstage",
				"sourcetitlebasicsstage lookup(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1"
			]
		}
	}
}