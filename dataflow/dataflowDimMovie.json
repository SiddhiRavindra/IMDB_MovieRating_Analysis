{
	"name": "dataflowDimMovie",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SnowflakeDimMovie",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTableDIMmovie",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey"
				},
				{
					"name": "derivedColumn"
				},
				{
					"name": "select"
				}
			],
			"scriptLines": [
				"parameters{",
				"     jobid as string (uuid())",
				"}",
				"source(output(",
				"          TITLE_BASICS_SK as decimal(38,0),",
				"          TCONST as string,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as decimal(10,0),",
				"          STARTYEAR as decimal(38,0),",
				"          ENDYEAR as decimal(38,0),",
				"          RUNTIMEMINUTES as decimal(38,0),",
				"          GENRES as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> source",
				"source keyGenerate(output(rownum as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey",
				"surrogateKey derive(DI_JOB_ID = $jobid,",
				"          DI_LOAD_DATE = currentTimestamp(),",
				"          rownum = rownum) ~> derivedColumn",
				"derivedColumn select(mapColumn(",
				"          MOVIE_SK = rownum,",
				"          TCONST,",
				"          TITLETYPE,",
				"          PRIMARYTITLE,",
				"          ORIGINALTITLE,",
				"          ISADULT,",
				"          STARTYEAR,",
				"          ENDYEAR,",
				"          RUNTIMEMINUTES,",
				"          GENRES,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DATE",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> select",
				"select sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          MOVIE_SK as decimal(10,0),",
				"          TCONST as string,",
				"          START_DATE_ID as date,",
				"          END_DATE_ID as date,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as string,",
				"          STARTYEAR as decimal(38,0),",
				"          ENDYEAR as decimal(38,0),",
				"          RUNTIMEMINUTES as decimal(38,0),",
				"          GENRES as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DATE as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     stageInsert: true,",
				"     mapColumn(",
				"          MOVIE_SK,",
				"          TCONST,",
				"          TITLETYPE,",
				"          PRIMARYTITLE,",
				"          ORIGINALTITLE,",
				"          ISADULT,",
				"          STARTYEAR,",
				"          ENDYEAR,",
				"          RUNTIMEMINUTES,",
				"          GENRES,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DATE",
				"     )) ~> sink"
			]
		}
	}
}